#!/bin/usr/python3
from models.base_model import BaseModel

class Amenity(BaseModel):
    "
    def __init__(self):
        super().__init__()
        self.name = ""

    def to_dict(self):
        amenity_dict = super().to_dict()
        amenity_dict.update({"name": self.name})
        return amenity_dict

    def from_dict(self, data):
        super().from_dict(data)
        self.name = data.get("name", "")
